{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"hellogroupa",
      "labels":{
         "app":"hello",
         "role":"groupa"
      }
   },
   "spec":{
      "replicas":1,
      "selector":{
        "app":"hello",
        "role":"groupa"
      },
      "template":{
         "metadata":{
            "labels":{
              "app":"hello",
              "role":"groupa"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"hello",
                  "image":"192.168.93.144:5000/yang/test:latest",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort":80
                     }
                  ]
               }
            ]
         }
      }
   }
}
